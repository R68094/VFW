Ti.UI.setBackgroundColor("#000");

var mainWindow = Ti.UI.createWindow({
	title: "My Main Window",
	backgroundColor: "#fff",
	backgroundRepeat: true
});

var myMainView = Ti.UI.createView({
	backgroundColor: "#000",
	height: 40,
	top: 30,
	left: 20,
	right: 20
});














mainWindow.add(myMainView);
mainWindow.open();
